{
  "name": "nuxt-mvvm",
  "version": "1.0.15",
  "description": "Intuitive, type safe and flexible MVVM implementation for nuxt based applications",
  "repository": {
    "url": "https://github.com/Fidden/nuxt-mvvm.git",
    "type": "git"
  },
  "author": {
    "name": "Alexandr Lisenkov",
    "url": "https://github.com/fidden"
  },
  "keywords": [
    "nuxt",
    "mvvm",
    "dependency-injection",
    "mvvm-architecture"
  ],
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "release": "pnpm lint && pnpm test && pnpm prepack && changelogen --release && pnpm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@nuxt/kit": "^3.7.0",
    "pinia": "^2.1.6",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxt/eslint-config": "^0.1.1",
    "@nuxt/module-builder": "^0.4.0",
    "@nuxt/schema": "^3.7.0",
    "@nuxt/test-utils": "^3.7.0",
    "@types/node": "^18.17.3",
    "changelogen": "^0.5.4",
    "eslint": "^8.46.0",
    "nuxt": "^3.7.4",
    "tsyringe": "^4.8.0",
    "vitest": "^0.34.1"
  }
}
